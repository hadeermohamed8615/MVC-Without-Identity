
@model List<Student>


<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Age</th>
            <th scope="col">Address</th>
            <th scope="col">Image</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var s in Model)
        {
        <tr>
            <th>@s.Id</th>
             <td>@s.Name</td>
             <td>@s.Age</td>
            <td>@s.Address</td>
            @*<td>@s.Image</td>*@
                <td><img style="width:50px;height:50px"src="~/Images/@s.Image" /></td>
        </tr>
        }
      
  </tbody>
 </table>
