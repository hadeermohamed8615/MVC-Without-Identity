
@model List<Department>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>




<br/>
<a href="/Department/New" class="btn btn-outline-secondary">New</a>
<br/>
<br/>

<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Location</th>
            <th scope="col">Manger</th>
  
        </tr>
    </thead>
    <tbody>
        @foreach (var d in Model)
        {
            <tr>
                <th>@d.Id</th>
                <td>@d.Name</td>
                <td>@d.Location</td>
                <td>@d.Manger</td>
             
            </tr>
        }

    </tbody>
</table>

